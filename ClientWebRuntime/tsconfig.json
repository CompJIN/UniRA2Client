{
  "include": [
    "src",
  ],
  "exclude": [
    "src/**/__tests__/*",
  ],
  "compilerOptions": {
    "target": "esnext",
    "strict": true,
    "allowJs": true,
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./src/*"
      ]
    },
    "types": [
      "node"
    ],
    "noEmit": true,
    "moduleResolution": "node"
  },
}